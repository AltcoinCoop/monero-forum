{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/SkepticalHippo/markdown"
        },
        {
            "type": "vcs",
            "url": "https://github.com/SkepticalHippo/php-markdown"
        }
    ],
	"require": {
		"laravel/framework": "4.2.*",
		"way/generators": "2.*",
		"fzaninotto/faker": "v1.3.0",
		"zizaco/entrust": "1.2.*@dev",
		"intervention/image": "2.*",
		"creitive/breadcrumbs": "dev-master",
		"guzzlehttp/guzzle": "~4.0",
		"roumen/feed": "dev-master#f76ffd73fc5eab35bfc6bf0029d195cde7691f89",
		"vtalbot/markdown": "dev-master",
		"michelf/php-markdown": "dev-lib"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
